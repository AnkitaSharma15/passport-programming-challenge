{
  "name": "passport-programming-challenge",
  "version": "0.1.0",
  "scripts": {
    "client": "cd client && npm start",
    "start": "node index.js",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "server": "nodemon --exec server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "mysql": "^2.15.0",
    "nodemon": "^1.17.5",
    "path": "^0.12.7",
    "serve": "^7.1.2",
    "socket.io": "^2.1.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
